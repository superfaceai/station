{
  "name": "@superfaceai/station",
  "version": "0.0.1",
  "description": "Superface Station utility",
  "main": "dist/src/index.js",
  "repository": "git@github.com:superfaceai/station.git",
  "author": "Superface Team",
  "private": true,
  "bin": {
    "station": "bin/station",
    "st": "bin/station"
  },
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.release.json --outDir dist",
    "clean": "rimraf dist/",
    "prebuild": "yarn clean",
    "test": "station compile && station check && dotenv -e .env.capabilities -- jest",
    "test:clean": "dotenv -e .env.capabilities -- jest --clear-cache && jest",
    "test:debug": "DEBUG=* yarn test",
    "lint": "eslint ./",
    "lint:fix": "yarn lint --fix",
    "format": "prettier -c ./",
    "format:fix": "prettier --write ./",
    "prepush": "yarn build && yarn lint:fix && yarn format:fix"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.15",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@superfaceai/ast": "0.0.23",
    "@superfaceai/cli": "^0.0.15",
    "@superfaceai/one-sdk": "0.0.22",
    "@superfaceai/parser": "0.0.16",
    "@superfaceai/service-client": "^0.0.14",
    "chalk": "^4.1.0",
    "dotenv-cli": "^4.0.0",
    "glob": "^7.1.7",
    "inquirer": "^7.3.3",
    "rimraf": "^3.0.2"
  },
  "oclif": {
    "commands": "dist/src/commands",
    "bin": "station"
  }
}
