{
  "name": "@superfaceai/station",
  "version": "0.0.1",
  "description": "Superface Station utility",
  "main": "dist/index.js",
  "repository": "git@github.com:superfaceai/station.git",
  "author": "Superface Team",
  "private": true,
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.release.json --outDir dist",
    "clean": "rimraf dist/",
    "prebuild": "yarn clean",
    "compile": "node dist/compile.js",
    "create:profile": "node dist/create-profile.js",
    "create:provider": "node dist/create-provider.js",
    "create:map": "node dist/create-map.js",
    "upload": "dotenv -e .env -- node dist/publish.js",
    "test": "yarn compile && dotenv -e .env.capabilities -- jest",
    "test:clean": "dotenv -e .env.capabilities -- jest --clear-cache && jest",
    "lint": "eslint ./",
    "lint:fix": "yarn lint --fix",
    "format": "prettier -c ./",
    "format:fix": "prettier --write ./",
    "prepush": "yarn build && yarn lint:fix && yarn format:fix"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.15",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^26.6.3",
    "nock": "^13.0.11",
    "prettier": "^2.2.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@superfaceai/ast": "0.0.22",
    "@superfaceai/brain-client": "^0.0.7",
    "@superfaceai/one-sdk": "0.0.20",
    "@superfaceai/parser": "0.0.15",
    "dotenv-cli": "^4.0.0",
    "rimraf": "^3.0.2"
  }
}
