{
  "name": "@superfaceai/station",
  "version": "0.0.0",
  "description": "Superface Station utility",
  "repository": "git@github.com:superfaceai/station.git",
  "author": "Superface Team",
  "scripts": {
    "build_reporter": "tsc reporter/*.ts --outDir dist",
    "tools:test": "jest -c jest.tools.config.js",
    "tools:lint": "eslint ./",
    "tools:lint:fix": "yarn tools:lint --fix",
    "tools:format": "prettier -c ./",
    "tools:format:fix": "prettier --write ./",
    "prepush": "yarn tools:lint:fix && yarn tools:format:fix",
    "test": "dotenv -e .env.example -- jest",
    "test:with_new_traffic": "USE_NEW_TRAFFIC=true yarn test",
    "test:update_traffic": "UPDATE_TRAFFIC=true yarn test",
    "test:record": "SUPERFACE_LIVE_API='*' dotenv -e .env -e .env.example -- jest",
    "test:record:dev": "TEST_ENV='dev' yarn test:record --group=live/safe",
    "test:record:prod": "TEST_ENV='prod' yarn test:record --group=live/safe",
    "test:update_snapshots": "yarn test -u",
    "check": "superface check",
    "lint": "superface lint",
    "structure": "ts-node ./src/station_structure.ts",
    "publish_to_registry": "ts-node ./src/publish.ts",
    "hosts:block": "ts-node ./src/hosts.ts block",
    "hosts:allow": "ts-node ./src/hosts.ts allow"
  },
  "devDependencies": {
    "@jest/reporters": "^28.1.3",
    "@jest/test-result": "^28.1.3",
    "@superfaceai/ast": "^1.2.0",
    "@superfaceai/cli": "^1.1.1",
    "@superfaceai/one-sdk": "^1.4.1",
    "@superfaceai/parser": "^1.2.0",
    "@superfaceai/testing": "2.1.0-beta.2",
    "@types/glob": "^7.1.4",
    "@types/jest": "^28.1.7",
    "@types/node": "^15.12.2",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "glob": "^7.1.7",
    "jest": "^28.1.3",
    "jest-runner-groups": "^2.2.0",
    "prettier": "^2.2.0",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.0.5"
  },
  "dependencies": {}
}
