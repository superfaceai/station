{
  "name": "slack",
  "services": [
    {
      "id": "api",
      "baseUrl": "https://slack.com/api"
    }
  ],
  "defaultService": "api",
  "securitySchemes": [
    {
      "id": "bearer_token",
      "type": "http",
      "scheme": "bearer"
    }
  ],
  "parameters": [
    {
      "name": "accessToken",
      "description": "User-authorized access token"
    }
  ]
}
