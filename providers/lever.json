{
  "name": "lever",
  "services": [
    {
      "id": "default",
      "baseUrl": "https://{SUBDOMAIN}.lever.co"
    }
  ],
  "securitySchemes": [
    {
      "id": "bearer_token",
      "type": "http",
      "scheme": "bearer"
    }
  ],
  "parameters": [
    {
      "name": "SUBDOMAIN",
      "description": "Use `api.sandbox` for sandbox environment, `api` for production (default)",
      "default": "api"
    }
  ],
  "defaultService": "default"
}
