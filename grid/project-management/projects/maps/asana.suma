profile = "project-management/projects@1.0"
provider = "asana"

map ListProjects {
  http GET "/projects" {
    security "personalAccessToken"
    
    
    response 200 "application/json" {
      return map result body.data.map((project) => {
        return {
          id: project.gid,
          name: project.name
        }
      })
    }
  }
}
