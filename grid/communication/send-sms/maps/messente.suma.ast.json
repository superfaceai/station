                          "deliveryStatus"
                        ],
                        "value": {
                          "kind": "JessieExpression",
                          "expression": "(function () {\n    switch (body.statuses[0].status) {\n        case 'ACK':\n        case 'ACCEPTD':\n            return 'accepted';\n        case 'DELIVRD':\n            return 'delivered';\n        case 'SEEN':\n            return 'seen';\n        case 'UNDELIV':\n        case 'FAILED':\n        case 'REJECTD':\n        case 'DELETED':\n        case 'EXPIRED':\n        case 'NACK':\n            return 'failed';\n        case 'UNKNOWN':\n        default:\n            return 'unknown';\n    }\n})()",
                          "source": "(() => {\n\t\t\t\t\tswitch (body.statuses[0].status) {\n\t\t\t\t\t\tcase 'ACK':\n\t\t\t\t\t\tcase 'ACCEPTD':\n\t\t\t\t\t\t\treturn 'accepted'\n\t\t\t\t\t\tcase 'DELIVRD':\n\t\t\t\t\t\t\treturn 'delivered';\n\t\t\t\t\t\tcase 'SEEN':\n\t\t\t\t\t\t\treturn 'seen';\n\t\t\t\t\t\tcase 'UNDELIV':\n\t\t\t\t\t\tcase 'FAILED':\n\t\t\t\t\t\tcase 'REJECTD':\n\t\t\t\t\t\tcase 'DELETED':\n\t\t\t\t\t\tcase 'EXPIRED':\n\t\t\t\t\t\tcase 'NACK':\n\t\t\t\t\t\t\treturn 'failed';\n\t\t\t\t\t\tcase 'UNKNOWN':\n\t\t\t\t\t\tdefault:\n\t\t\t\t\t\t\treturn 'unknown'\n\t\t\t\t\t}\n\t\t\t\t})()",
