        "twilio": {
          "file": "./grid/communication/send-sms/maps/twilio.suma",
          "defaults": {
            "SendMessage": {
              "input": {
                "from": "$TWILIO_COMMUNICATION_SENDMESSAGE_FROM",
                "TWILIO_SID": "$TWILIO_USERNAME"
              }
            },
            "RetrieveMessageStatus": {
              "input": {
                "from": "$TWILIO_COMMUNICATION_SENDMESSAGE_FROM",
