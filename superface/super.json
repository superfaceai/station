        }
      }
    },
    "delivery-tracking/shipment-info": {
      "file": "grid/delivery-tracking/shipment-info/profile.supr",
      "defaults": {},
      "providers": {
        "dhl-unified": {
          "file": "./grid/delivery-tracking/shipment-info/maps/dhl-unified.suma"
        },
        "shippo": {
          "file": "./grid/delivery-tracking/shipment-info/maps/shippo.suma"
        },
        "mock": {
          "file": "./grid/delivery-tracking/shipment-info/maps/mock.suma"
        }
      }
