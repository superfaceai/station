{
  "profiles": {
    "communication/send-message": {
      "file": "./grid/communication/send-message/profile.supr",
      "providers": {
        "twilio": {
          "file": "./grid/communication/send-message/maps/twilio.suma",
          "defaults": {
            "SendMessage": {
              "input": {
                "from": "$TWILIO_COMMUNICATION_SENDMESSAGE_FROM",
                "TWILIO_SID": "$TWILIO_USERNAME"
              }
            },
            "RetrieveMessageStatus": {
              "input": {
                "from": "$TWILIO_COMMUNICATION_SENDMESSAGE_FROM",
                "TWILIO_SID": "$TWILIO_USERNAME"
              }
            }
          }
        },
        "tyntec": {
          "file": "./grid/communication/send-message/maps/tyntec.suma",
          "defaults": {
            "SendMessage": {
              "input": {
                "from": "tyntec"
              }
            }
          }
        },
        "vonage-nexmo": {
          "file": "./grid/communication/send-message/maps/vonage-nexmo.suma",
          "defaults": {
            "SendMessage": {
              "input": {
                "NEXMO_API_KEY": "$NEXMO_API_KEY",
                "NEXMO_API_SECRET": "$NEXMO_API_SECRET"
              }
            },
            "RetrieveMessageStatus": {
              "input": {
                "NEXMO_API_KEY": "$NEXMO_API_KEY",
                "NEXMO_API_SECRET": "$NEXMO_API_SECRET"
              }
            }
          }
        },
        "plivo": {
          "file": "./grid/communication/send-message/maps/plivo.suma",
          "defaults": {
            "SendMessage": {
              "input": {
                "PLIVO_AUTH_ID": "$PLIVO_AUTH_ID",
                "PLIVO_AUTH_TOKEN": "$PLIVO_AUTH_TOKEN"
              }
            },
            "RetrieveMessageStatus": {
              "input": {
                "PLIVO_AUTH_ID": "$PLIVO_AUTH_ID",
                "PLIVO_AUTH_TOKEN": "$PLIVO_AUTH_TOKEN"
              }
            }
          }
        }
      }
    },
    "vcs/user-repos": {
      "file": "./grid/vcs/user-repos/profile.supr",
      "providers": {
        "github": {
          "file": "./grid/vcs/user-repos/maps/github.suma"
        },
        "gitlab": {
          "file": "./grid/vcs/user-repos/maps/gitlab.suma"
        },
        "bitbucket": {
          "file": "./grid/vcs/user-repos/maps/bitbucket.suma"
        }
      }
    },
    "vcs/pull-requests": {
      "file": "./grid/vcs/pull-requests/profile.supr",
      "providers": {
        "gitlab": {
          "file": "./grid/vcs/pull-requests/maps/gitlab.suma"
        },
        "bitbucket": {
          "file": "./grid/vcs/pull-requests/maps/bitbucket.suma"
        },
        "github": {
          "file": "./grid/vcs/pull-requests/maps/github.suma"
        }
      }
    },
    "vcs/single-file-content": {
      "file": "./grid/vcs/single-file-content/profile.supr",
      "providers": {
        "github": {
          "file": "./grid/vcs/single-file-content/maps/github.suma"
        },
        "gitlab": {
          "file": "./grid/vcs/single-file-content/maps/gitlab.suma"
        },
        "bitbucket": {
          "file": "./grid/vcs/single-file-content/maps/bitbucket.suma"
        }
      }
    },
    "address/clean-address": {
      "file": "./grid/address/clean-address/profile.supr",
      "providers": {
        "smartystreets": {
          "file": "./grid/address/clean-address/maps/smartystreets.suma",
          "defaults": {
            "CleanAddress": {
              "input": {
                "SMARTYSTREETS_AUTH_ID": "$SMARTYSTREETS_AUTH_ID"
              }
            }
          }
        }
      }
    },
    "communication/send-email": {
      "file": "./grid/communication/send-email/profile.supr",
      "providers": {
        "sendgrid": {
          "file": "./grid/communication/send-email/maps/sendgrid.suma",
          "defaults": {
            "SendEmail": {
              "input": {
                "from": "$SENDGRID_COMMUNICATION_SENDEMAIL_FROM",
                "to": "$COMMUNICATION_SENDEMAIL_TO"
              }
            }
          }
        }
      }
    },
    "vcs/pull-request": {
      "file": "./grid/vcs/pull-request/profile.supr",
      "providers": {
        "github": {
          "file": "./grid/vcs/pull-request/maps/github.suma"
        },
        "gitlab": {
          "file": "./grid/vcs/pull-request/maps/gitlab.suma"
        },
        "bitbucket": {
          "file": "./grid/vcs/pull-request/maps/bitbucket.suma"
        }
      }
    },
    "address/geocoding": {
      "file": "./grid/address/geocoding/profile.supr",
      "providers": {
