            "RetrieveMessageStatus": {
              "input": {
                "PLIVO_AUTH_ID": "$PLIVO_AUTH_ID",
                "PLIVO_AUTH_TOKEN": "$PLIVO_AUTH_TOKEN"
              }
            }
          }
        },
        "mock": {
          "file": "./grid/communication/send-sms/maps/mock.suma"
        }
      }
