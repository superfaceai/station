    "fintech/exchange-rate": {
      "file": "./grid/fintech/exchange-rate/profile.supr",
      "providers": {
        "fixer": {
          "file": "./grid/fintech/exchange-rate/maps/fixer.suma"
        },
        "openexchangerates": {
          "file": "./grid/fintech/exchange-rate/maps/openexchangerates.suma"
        },
        "mock": {
          "file": "./grid/fintech/exchange-rate/maps/mock.suma"
        }
      }
    }
  },
  "providers": {
    "github": {
      "file": "../providers/github.json",
      "security": []
    },
    "twilio": {
      "file": "../providers/twilio.json",
      "security": [
        {
          "id": "basic",
          "username": "$TWILIO_USERNAME",
          "password": "$TWILIO_PASSWORD"
        }
      ]
    },
    "tyntec": {
      "file": "../providers/tyntec.json",
      "security": [
        {
          "id": "apikey",
          "apikey": "$TYNTEC_API_KEY"
        }
      ]
    },
    "gitlab": {
      "file": "../providers/gitlab.json",
      "security": []
    },
    "bitbucket": {
      "file": "../providers/bitbucket.json",
      "security": [
        {
          "id": "basic",
          "username": "$BITBUCKET_USERNAME",
          "password": "$BITBUCKET_PASSWORD"
        }
      ]
    },
    "smartystreets": {
      "file": "../providers/smartystreets.json",
      "security": [
        {
          "id": "auth-token",
          "apikey": "$SMARTYSTREETS_API_KEY"
        }
      ]
    },
    "vonage-nexmo": {
      "file": "../providers/vonage-nexmo.json",
      "security": []
    },
    "plivo": {
      "file": "../providers/plivo.json",
      "security": [
        {
          "id": "basic",
          "username": "$PLIVO_AUTH_ID",
          "password": "$PLIVO_AUTH_TOKEN"
        }
      ]
    },
    "dhl-unified": {
      "file": "../providers/dhl-unified.json",
      "security": [
        {
          "id": "dhl_api_key",
          "apikey": "$DHL_API_KEY"
        }
      ]
    },
    "sendgrid": {
      "file": "../providers/sendgrid.json",
      "security": [
        {
          "id": "bearer_token",
          "token": "$SENDGRID_API_KEY"
        }
      ]
    },
    "google-apis": {
      "file": "../providers/google-apis.json",
      "security": [
        {
          "id": "apikey",
          "apikey": "$GOOGLE_MAPS_API_KEY"
        }
      ]
    },
    "opencage": {
      "file": "../providers/opencage.json",
      "security": [
        {
          "id": "apikey",
          "apikey": "$OPENCAGE_API_KEY"
        }
      ]
    },
    "shippo": {
      "file": "../providers/shippo.json",
      "security": [
        {
          "id": "shippo_api_token",
          "apikey": "$SHIPPO_TOKEN"
        }
      ]
    },
    "mock": {
      "file": "../providers/mock.json",
      "security": []
    },
    "fixer": {
      "file": "../providers/fixer.json",
      "security": [
        {
          "id": "apikey_auth",
          "apikey": "$FIXER_API_KEY"
        }
      ]
    },
    "openexchangerates": {
      "file": "../providers/openexchangerates.json",
      "security": [
        {
          "id": "apikey_auth",
          "apikey": "$OE_API_KEY"
        }
      ]
    }
  }
}