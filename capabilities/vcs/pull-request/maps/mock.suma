profile = "vcs/pull-request@1.0"
provider = "mock"

"""
PullRequest mock map
"""
map PullRequest{
  identifier = input.identifier
  map result {
        title: 'Test',
        id: identifier,
        url: `https://gitlab.com/${input.owner}/${input.repo}/-/merge_requests/${input.identifier}`,
        sha: Math.random().toString(16),
      }
}
